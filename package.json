{
  "name": "coin-box-ethereum",
  "version": "1.0.0",
  "description": "Ethereum test project",
  "main": "index.js",
  "scripts": {
    "start": "npm run compile",
    "compile": "truffle compile",
    "test": "truffle test --network test",
    "test-network": "node lib/start-test-environment.js",
    "local-ethereum-network": "node local-ethereum/run-network.js",
    "deploy-to-test-network": "truffle migrate --reset --network test_network",
    "deploy-to-local-ethereum-network": "truffle migrate --reset --network local_etherium",
    "connect-to-local-ethereum-network": "node local-ethereum/connect-to-network.js",
    "dev": "node --inspect --inspect-brk node_modules/truffle/build/cli.bundled.js migrate --reset --network local_etherium"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PavelDymkov/coin-box-ethereum.git"
  },
  "keywords": [],
