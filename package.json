  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PavelDymkov/coin-box-ethereum/issues"
  },
  "homepage": "https://github.com/PavelDymkov/coin-box-ethereum",
  "dependencies": {
    "ganache-core": "^2.0.2",
    "truffle": "^4.0.1",
    "web3": "^1.0.0-beta.26",
    "zeppelin-solidity": "^1.4.0"
  }
}
